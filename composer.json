{
    "name": "boonestudios/laravel-surrealdb",
    "description": "Integrates SurrealDB with the Laravel framework",
    "keywords": [
        "boonestudios",
        "laravel-surrealdb"
    ],
    "homepage": "https://github.com/boonestudios/laravel-surrealdb",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Boone Studios, LLC",
            "email": "support@boonestudios.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Boonestudios\\LaravelSurrealdb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boonestudios\\LaravelSurrealdb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Boonestudios\\LaravelSurrealdb\\LaravelSurrealdbServiceProvider"
            ],
            "aliases": {
                "LaravelSurrealdb": "Boonestudios\\LaravelSurrealdb\\LaravelSurrealdbFacade"
            }
        }
    }
}
